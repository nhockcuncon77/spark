{
  "expo": {
    "name": "Spark",
    "slug": "spark",
    "version": "0.6.10",
    "orientation": "portrait",
    "icon": "./assets/main-dark.png",
    "scheme": "spark",
    "userInterfaceStyle": "automatic",
    "platforms": ["web"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/main-dark.png"
    },
    "plugins": [
      "expo-router",
      ["expo-splash-screen", { "backgroundColor": "#0B0223" }],
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "baseUrl": "/app"
    },
    "extra": {
      "router": {},
      "eas": { "projectId": "bda153aa-b6d3-4b06-9c0f-a28402588af1" }
    }
  }
}
